ed error.":["\u0420\u0435\u0434\u0430\u043a\u0442\u043e\u0440 \u043a\u04af\u0442\u04af\u043b\u0431\u04e9\u0433\u04e9\u043d \u043a\u0430\u0442\u0430\u0433\u0430 \u0442\u0443\u0448 \u0431\u043e\u043b\u0434\u0443."],"Copy Error":["\u041a\u0430\u0442\u0430 \u0442\u0435\u043a\u0441\u0442\u0438\u043d \u043a\u04e9\u0447\u04af\u0440\u04af\u04af"],"(opens in a new tab)":["(\u0436\u0430\u04a3\u044b \u04e9\u0442\u043c\u04e9\u043a\u0442\u04e9 \u0430\u0447\u044b\u043b\u0430\u0442)"],"Customizing":["\u0416\u04e9\u043d\u0434\u04e9\u04e9"],"Tools":["\u0410\u0441\u043f\u0430\u043f\u0442\u0430\u0440"],"Help":["\u0416\u0430\u0440\u0434\u0430\u043c"],"Widgets":["\u0412\u0438\u0434\u0436\u0435\u0442\u0442\u0435\u0440"],"Redo":["\u041a\u0430\u0439\u0442\u0430\u043b\u043e\u043e"],"Undo":["\u0416\u043e\u043a\u043a\u043e \u0447\u044b\u0433\u0430\u0440\u0443\u0443"]}},"comment":{"reference":"wp-includes\/js\/dist\/customize-widgets.js"}}